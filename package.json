{
    "name": "visual-sort",
    "version": "0.0.1",
    "description": "A sorting visualization to exemplify multiple types of sorting algorithms",
    "private": true,
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "webpack serve -open",
        "build": "webpack",
        "check": "tsc --noEmit && eslint . && prettier --check **/*.ts **/*.js **/*.css",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write **/*.ts **/*.js **/*.css"
    },
    "babel": {
        "presets": [
            "@babel/preset-env"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/SamHaggans/visual-sort.git"
    },
    "author": "SamHaggans",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.17.5",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-typescript": "^7.6.0",
        "@types/lodash": "^4.14.179",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "babel-loader": "^8.2.3",
        "css-loader": "^6.6.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.4.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.2.0",
        "prettier": "^2.5.1",
        "style-loader": "^3.3.1",
        "ts-node": "^10.5.0",
        "typescript": "^4.5.5",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.7.4"
    },
    "dependencies": {
        "lodash": "^4.17.21"
    }
}
